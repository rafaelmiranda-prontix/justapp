{
  "name": "mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "flutter run --dart-define USE_SUPABASE=${USE_SUPABASE:-false} --dart-define API_URL=${API_URL:-http://10.0.2.2:3000/api} --dart-define SUPABASE_URL=${SUPABASE_URL:-} --dart-define SUPABASE_ANON_KEY=${SUPABASE_ANON_KEY:-}",
    "build:android": "flutter build apk --release",
    "build:ios": "flutter build ios --release",
    "test": "flutter test",
    "lint": "flutter analyze",
    "clean": "flutter clean",
    "codegen": "flutter pub run build_runner build --delete-conflicting-outputs"
  }
}
